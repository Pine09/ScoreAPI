                },
                "password": {
                    "type": "string"
                }
            }
        },
        "mahasiswa": {
            "properties": {
                "NIM": {
                    "type": "integer",
                    "format": "int32"
                },
                "nama_depan": {
                    "type": "string"
                },
                "nama_belakang": {
                    "type": "string"
                },
                "email": {
                    "type": "string"
                },
                "alamat": {
                    "type": "string"
                },
                "jenis_kelamin": {
                    "type": "string"
                }
            }
        },
        "dosen": {
            "properties": {
                "NID": {
                    "type": "integer",
                    "format": "int32"
                },
                "nama_depan": {
                    "type": "string"
                },
                "nama_belakang": {
                    "type": "string"
                },
                "email": {
                    "type": "string"
                },
                "alamat": {
                    "type": "string"
                },
                "jenis_kelamin": {
                    "type": "string"
                }
            }
        },
        "jadwal": {
            "properties": {
                "dosen_id": {
                    "type": "integer",
                    "format": "int32"
                },
                "matkul_id": {
                    "type": "integer",
                    "format": "int32"
                },
                "day": {
                    "type": "string"
                },
                "status": {
                    "type": "string"
                }
            }
        },
        "nilai": {
            "properties": {
                "mahasiswa_id": {
                    "type": "integer",
                    "format": "int32"
                },
                "jadwal_id": {
