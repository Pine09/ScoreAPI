                },
                "assignment": {
                    "type": "integer",
                    "format": "int32"
                },
                "UTS": {
                    "type": "integer",
                    "format": "int32"
                },
                "UAS": {
                    "type": "integer",
                    "format": "int32"
                }
            }
        }
    }
}